{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }, 
        "console": {
            "isEnabled": true,
            "loggingLevel": {
                "default": "Information",
                "Host.Results": "Error",
                "Function": "Information",
                "Host.Aggregator": "Trace"
            }
        }
    }
}